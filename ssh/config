# don't check host keys on .local
Host *.local
    IdentityFile ~/.ssh/id_rsa
    UseKeychain yes
    User jefarr
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null ~/.ssh/known_hosts

Host 192.168.*
    IdentityFile ~/.ssh/id_rsa
    UseKeychain yes
    User jefarr
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null ~/.ssh/known_hosts

Host norns.local
    IdentityFile ~/.ssh/id_rsa
    User we
